{
	"name": "reactnd-project-myreads",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "react-app-rewired build",
		"commit": "git-cz",
		"eject": "react-app-rewired eject",
		"eslint-check": "eslint --print-config .eslintrc.js",
		"format": "prettier-eslint \"src/**/*.js\" --write",
		"precommit": "lint-staged",
		"start": "react-app-rewired start",
		"test": "react-app-rewired test --env=jsdom"
	},
	"dependencies": {
		"react": "^16.3.0",
		"react-dom": "^16.3.0",
		"react-scripts": "1.1.1"
	},
	"devDependencies": {
		"commitizen": "^2.9.6",
		"cz-conventional-changelog": "^2.1.0",
		"eslint": "^4.19.1",
		"eslint-config-prettier": "^2.9.0",
		"eslint-config-pretty": "^2.1.1",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-eslint-comments": "^2.0.2",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-json": "^1.2.0",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-promise": "^3.7.0",
		"eslint-plugin-sort-class-members": "^1.3.0",
		"eslint-plugin-unicorn": "^4.0.3",
		"husky": "^0.14.3",
		"lint-staged": "^7.0.2",
		"prettier-eslint": "^8.8.1",
		"prettier-eslint-cli": "^4.7.1",
		"react-app-rewire-eslint": "^0.2.3",
		"react-app-rewired": "^1.5.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		"*.js": ["eslint --fix"],
		"*.{js,json,css,scss,sass,md}": [
			"prettier-eslint --eslint-config-path .eslintrc.js --config .prettierrc --list-different --write",
			"git add"
		]
	}
}
